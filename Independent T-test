import pandas as pd
from scipy import stats
from scipy.stats import t

url2019f = 'https://raw.githubusercontent.com/VWTZLS/First-Year-Project/main/2019%20females.csv'
f2019 = pd.read_csv(url2019f)
url2019m = 'https://raw.githubusercontent.com/VWTZLS/First-Year-Project/main/2019%20males.csv'
m2019 = pd.read_csv(url2019m)

url2020f = 'https://raw.githubusercontent.com/VWTZLS/First-Year-Project/main/2020%20females.csv'
f2020 = pd.read_csv(url2020f)
url2020m = 'https://raw.githubusercontent.com/VWTZLS/First-Year-Project/main/2020%20males.csv'
m2020 = pd.read_csv(url2020m)

url2021f = 'https://raw.githubusercontent.com/VWTZLS/First-Year-Project/main/2021%20females.csv'
f2021 = pd.read_csv(url2021f)
url2021m = 'https://raw.githubusercontent.com/VWTZLS/First-Year-Project/main/2021%20males.csv'
m2021 = pd.read_csv(url2021m)

test2019 = stats.ttest_ind(m2019, f2019, equal_var = False)
test2020 = stats.ttest_ind(m2020, f2020, equal_var = False)
test2021 = stats.ttest_ind(m2021, f2021, equal_var = False)
print("2019-2020 t-statistic and p-value:", test2019)
print("2020-2021 t-statistic and p-value:", test2020)
print("2021-2022 t-statistic and p-value:", test2021)
